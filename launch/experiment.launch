<?xml version="1.0" ?>
<launch>
	<arg name="shuttle"  default = "1.0"/>
	<node name="stereo_record_node" pkg="flea3" type="stereo_record_node" output="screen" args="$(arg shuttle)" >

	</node>

	<node pkg="rosbag" type="record" name="rosbag_record_cam" required="true" output="screen"
	args="-o $(env HOME)/stereo_cam -a" />

	<node name="gps" pkg="flea3" type="rosNovatel" required="true" output="screen">
		<param name="port" type="str" value="/dev/ttyUSB0" />
		<param name="baudrate" type="int" value="115200" />
		<param name="gps_default_cutoff" type="double" value="15.0" />

		<param name="use_bestpos" type="bool" value="true" />
		<param name="use_bestvel" type="bool" value="false" />
		<param name="use_inscov" type="bool" value="true" />
		<param name="use_inspva" type="bool" value="true" />
		<param name="use_insspd" type="bool" value="true" />

		<param name="bestpos_freq" type="double" value="20.0" />
		<param name="bestvel_freq" type="double" value="20.0" />
		<param name="inscov_freq" type="double" value="1.0" />
		<param name="inspva_freq" type="double" value="100.0" />
		<param name="insspd_freq" type="double" value="20.0" />

	</node>
</launch>
